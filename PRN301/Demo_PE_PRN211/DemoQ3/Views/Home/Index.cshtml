@using DemoQ3.Models;

@model List<Student>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DemoQ3</title>
    </head>
    <body>
    <h1>List Student</h1>
    @*@Html.DropDownListFor(model => Model., new SelectList(Model.Items, "Value", "Text"), "Please select an item")*@
    <table>
        <thead>
            <tr>
                <th>FullName</th>
                <th>Sex</th>
                <th>Dob</th>
                <th>Email</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var list in Model)
            {
                <tr>
                    <td>@list.Fullname </td>
                    <td>@list.Sex</td>
                    <td>@list.Dob</td>
                    <td>@list.Email</td>
                </tr>
            }

        </tbody>
    </table>
    </body>
</html>

